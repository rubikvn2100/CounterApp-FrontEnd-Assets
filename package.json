{
  "scripts": {
    "build": "webpack --mode production",
    "clean": "rm -rf dist",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "format": "npm run format:package-json && npm run prettier:fix",
    "format:package-json": "sort-package-json && prettier --write package.json",
    "lint": "eslint '*/**/*.{ts,tsx}' && npm run prettier:check",
    "prettier:check": "prettier --check '*/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write '*/**/*.{ts,tsx}'",
    "start": "webpack serve --mode development",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.14",
    "@types/react-test-renderer": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "eslint": "^8.51.0",
    "html-webpack-plugin": "^5.5.3",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "^18.2.0",
    "sort-package-json": "^2.6.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
